---
import BaseHead from "@/components/BaseHead.astro";
interface Props {
  title: string;
}

const { title } = Astro.props;
---
<!doctype html>
<html lang="en">
	<head>
        <BaseHead />
		<title>{ title }</title>
        <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;700&display=swap" rel="stylesheet">
	</head>
	<body>
		<slot />
	</body>
</html>